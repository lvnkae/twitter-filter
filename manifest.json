{
   "background": {
      "persistent": true,
      "scripts": [ "js/url_wrapper.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/badge_on.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "Twitter Filter",
      "name": "Twitter Filter"
   },
   "content_scripts": [ {
      "js": [ "js/jquery-3.3.1.min.js", "js/text_utility.js", "js/url_wrapper.js", "js/fixed_filter.js", "js/storage_data.js", "js/content.js" ],
      "matches": [ "https://twitter.com/*"],
      "run_at": "document_start"
   } ],
   "description": "Twitter,Togetter,Yahoo!リアルタイム検索にフィルタ機能を追加するアドオン/拡張機能です。ログインせずに各種ミュートが行えます。",
   "icons": {
      "19": "img/badge_on.png",
      "48": "img/icon_48.png"
   },
   "manifest_version": 2,
   "name": "Twitterフィルタ",
   "permissions": [ "tabs", "storage", "webRequest",
                    "https://amba.to/*",
                    "https://amzn.to/*",
                    "https://bit.ly/*",
                    "https://buff.ly/*",
                    "https://dlvr.it/*",
                    "https://goo.gl/*",
                    "https://htn.to/*", "https://b.hatena.ne.jp/-/*",
                    "https://ino.to/*",
                    "https://ift.tt/*",
                    "https://is.gd/*",
                    "https://j.mp/*",
                    "https://kisu.me/*",
                    "https://lb.to/*",
                    "http://nav.cx/*",
                    "https://npx.me/*",
                    "http://ow.ly/*",
                    "https://tinyurl.com/*" ],
   "short_name": "TwitterFilter",
   "version": "1.0.0.0"
}
